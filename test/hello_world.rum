#test
hello_world :: () {
  vagrants: [u8] = [10,10,10,104,101,108,108,111,32,119,111,114,108,100, 32, 240, 159, 152, 138, 33, 10,10,10]
  syscall_write(vagrants)

}

#linux-x86
syscall_write :: (str : [u8]) { 
  ASM {
    rdi = 2, RSI = str, RDX = str.#len, RAX = 1
    // https://www.felixcloutier.com/x86/syscall
    : 0F 05     // Syscall
  } 
}

