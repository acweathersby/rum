aligned :: (offset: u32, alignment: u32) : u32

  (offset + ( alignment + 1 )) & (4294967295 - (alignment - 1))


max :: (a:u32, b:u32) : u32 

  if a > b { a } otherwise { b }


min :: (a:u32, b:u32) : u32 

  if a < b { a } otherwise { b }


